{
  "name": "ntfs-tool",
  "version": "2.3.5-free",
  "description": "Free and open-source NTFS & ExFAT Tool for macOS - Community fork with original author permission",
  "homepage": "https://github.com/ntfstool/ntfstool",
  "author": {
    "name": "Dr_rOot (Original Author) & Community Contributors",
    "email": "service@ntfstool.com"
  },
  "copyright": "Copyright© 2018-present Dr_rOot & Community Contributors",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "e2e": "npm run pack && mocha test/e2e",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --config .electron-vue/webpack.renderer.config.js",
    "test": "npm run unit && npm run e2e",
    "unit": "karma start test/unit/karma.conf.js",
    "postinstall": "",
    "rebuild": "electron-rebuild -f -w usb"
  },
  "build": {
    "productName": "NTFSTool",
    "appId": "com.ntfstool.aile",
    "asarUnpack": [
      "**/ntfstool/node_modules/ntfstool/*"
    ],
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "publish": [
      {
        "provider": "github",
        "owner": "ntfstool",
        "repo": "ntfstool"
      }
    ],
    "mac": {
      "icon": "build/icons/icon.icns",
      "extendInfo": {
        "LSUIElement": 1,
        "Application Category": "Utilities",
        "Copyright(All)": "Copyright © 2018-present Dr_rOot & Community Contributors. All rights reserved.",
        "NSUserNotificationAlertStyle": "alert"
      }
    }
  },
  "dependencies": {
    "@electron/remote": "^2.1.3",
    "axios": "^1.7.9",
    "diskutil": "latest",
    "electron-log": "^5.2.2",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.3.9",
    "element-plus": "^2.9.1",
    "get": "^1.4.0",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "node-cache": "^5.1.2",
    "ntfstool": "latest",
    "usb": "^2.16.0",
    "vue": "^3.5.13",
    "vue-i18n": "^10.0.5",
    "vue-loader": "^17.4.2",
    "vue-router": "^4.5.0",
    "vuex": "^4.1.0",
    "watch-mac": "latest"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.26.0",
    "@babel/register": "^7.25.9",
    "@electron/rebuild": "^3.7.2",
    "@vue/compiler-sfc": "^3.5.13",
    "babel-loader": "^9.2.1",
    "babel-plugin-istanbul": "^7.0.0",
    "cfonts": "^3.3.0",
    "chai": "^4.5.0",
    "chalk": "^4.1.2",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "del": "^6.1.1",
    "devtron": "^1.4.0",
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8",
    "electron-debug": "^4.0.2",
    "electron-devtools-installer": "^3.2.0",
    "html-webpack-plugin": "^5.6.3",
    "karma": "^6.4.4",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^2.2.1",
    "karma-electron": "^7.3.0",
    "karma-mocha": "^2.0.1",
    "karma-sourcemap-loader": "^0.4.0",
    "karma-spec-reporter": "^0.0.36",
    "karma-webpack": "^5.0.1",
    "mini-css-extract-plugin": "^2.9.2",
    "mocha": "^10.8.2",
    "multispinner": "^0.2.1",
    "node-loader": "^2.0.0",
    "prettier": "^3.4.2",
    "require-dir": "^1.2.0",
    "sass": "^1.83.0",
    "sass-loader": "^16.0.4",
    "spectron": "^19.0.0",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.11",
    "vue-html-loader": "^1.2.4",
    "vue-style-loader": "^4.1.3",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.0",
    "webpack-hot-middleware": "^2.26.1",
    "webpack-merge": "^6.0.1"
  }
}
